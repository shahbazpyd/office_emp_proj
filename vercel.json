{
  "version": 2,
  "builds": [
    {
      "src": "office_emp_proj/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "installCommand": "pip install -r requirements.txt", 
        "buildCommand": "python manage.py migrate --noinput && python manage.py collectstatic --noinput --clear"
      }
    }
  ],
  "routes": [
    { 
      "src": "/static/(.*)", 
      "dest": "/static/$1" 
    },
    { "src": "/(.*)", "dest": "office_emp_proj/wsgi.py" }
  ]
}