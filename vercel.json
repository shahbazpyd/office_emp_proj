{
  "builds": [
    {
      "src": "office_emp_proj/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "build": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "office_emp_proj/wsgi.py" }
  ]
}
