
{
  "version": 2,
  "builds": [
  
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "maxLambdaSize": "15mb",
        "outputDirectory": "staticfiles" 
      }
    },

    {
      "src": "office_emp_proj/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
   
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1" 
    },
   
    {
      "src": "/(.*)",
      "dest": "office_emp_proj/wsgi.py"
    }
  ]
}