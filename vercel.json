{
  "version": 2,
  "builds": [
    {
      "src": "office_emp_proj/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "office_emp_proj/wsgi.py" }
  ]
}